@{
    ViewBag.Title = "Index";
}

<div id='jqxTabs'>
    <ul>
        <li style="margin-left: 30px;">Мониторинг</li>
        <li>Отчёт</li>    
    </ul>
    <div>
        Мониторинг
    </div>
    <div>
        Отчёт
        <div id='jqxDateTimeInput'></div>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        // Create jqxTabs.
        $('#jqxTabs').jqxTabs({ width: '100%', height: '500', position: 'top' });

        $("#jqxDateTimeInput").jqxDateTimeInput({ min: new Date(2010, 0, 1), max: new Date(2020, 11, 31), width: '250px', height: '25px' }); 
        
    });
    </script>
